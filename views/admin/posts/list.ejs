<div id="page_content">
    <div id="page_content_inner">
      <div class="md-card uk-margin-medium-bottom">
        <div class="md-card-content">
          <div class="uk-overflow-container">
            <table class="uk-table uk-table-nowrap table_check">
              <thead>
              <%- partial('../partials/flash.ejs') %>
              <tr>
                <th class="uk-width-2-10">عنوان</th>
                <th class="uk-width-1-10 uk-text-center">تاریخ ثبت</th>
                <th class="uk-width-1-10 uk-text-center">تاریخ بروزرسانی</th>
                <th class="uk-width-1-10 uk-text-center">دسته بندی</th>
                <th class="uk-width-1-10 uk-text-center">منتشر شده</th>
                <th class="uk-width-2-10 uk-text-center">عملیات ها</th>
              </tr>
              </thead>
              <tbody>

              <% posts.forEach( post =>{ %>

              <tr>
                <td><%= post.title %></td>
                <td class="uk-text-center"><%= new Date(post.createdAt) %></td>
                <td class="uk-text-center"><%= new Date(post.updatedAt) %></td>
                <td><%= post.category.name %></td>
                <% if (post.published) { %>
                <td class="uk-text-center"><span class="uk-badge">بله</span></td>
                <% } else {%>
                <td class="uk-text-center"><span class="uk-badge uk-badge-danger">خیر</span></td>
                <% } %>
                <td class="uk-text-center">
                  <a href="/admin/posts/edit/<%= post.id %>"><i class="md-icon material-icons">&#xE254;</i></a>
                  <a href="/admin/posts/delete/<%= post.id %>"><i class="md-icon material-icons">delete</i></a>
                </td>
              </tr>

              <% }); %>
              </tbody>
            </table>
          </div>
        </div>
        </div>
    </div>
